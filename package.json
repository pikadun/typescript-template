{
  "name": "web-template",
  "version": "0.0.1",
  "description": "Server-first HTML rendering stack with NestJS and Rsbuild",
  "type": "module",
  "scripts": {
    "dev": "node --experimental-strip-types --experimental-vm-modules scripts/start.ts",
    "build": "rsbuild build --config ./scripts/rsbuild.config.ts --mode production",
    "build:dev": "rsbuild build --config ./scripts/rsbuild.config.ts --mode development",
    "commitlint": "commitlint --config ./eng/.commitlintrc.json --edit",
    "cspell": "cspell --config ./eng/cspell.config.json --quiet .",
    "eslint": "eslint --config ./eng/eslint.config.js .",
    "lint": "npm run cspell && npm run eslint",
    "lint:staged": "lint-staged --config ./eng/.lintstagedrc.json",
    "postinstall": "husky || true",
    "type-check": "tsc --build ./tsconfig.json"
  },
  "author": "Dunn",
  "license": "MIT",
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-fastify": "^11.1.12",
    "@sequelize/core": "^7.0.0-alpha.47",
    "@sequelize/sqlite3": "^7.0.0-alpha.47",
    "@vue/server-renderer": "^3.5.27"
  },
  "devDependencies": {
    "@camaro/eslint-config": "~0.1.1",
    "@camaro/tsconfig": "^1.2.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-vue": "^1.2.3",
    "@types/node": "^24.0.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "cspell": "^9.6.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vue": "^3.5.27"
  }
}
