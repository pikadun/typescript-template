{
  "name": "web-template",
  "version": "0.0.1",
  "description": "Server-first HTML rendering stack with NestJS and Rsbuild",
  "type": "module",
  "scripts": {
    "dev": "node --experimental-strip-types scripts/start.ts",
    "build": "BUILD_ENV=production rsbuild build --config ./scripts/rsbuild.config.ts",
    "commitlint": "commitlint --config ./eng/.commitlintrc.json --edit",
    "cspell": "cspell --config ./eng/cspell.config.json --quiet .",
    "eslint": "eslint --config ./eng/eslint.config.js .",
    "lint": "npm run cspell && npm run eslint",
    "lint:staged": "lint-staged --config ./eng/.lintstagedrc.json",
    "postinstall": "husky || true",
    "type-check": "tsc --build ./tsconfig.json"
  },
  "author": "Dunn",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/sequelize": "^11.0.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@camaro/eslint-config": "~0.1.0",
    "@camaro/tsconfig": "^1.2.0",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@rsbuild/core": "^1.7.1",
    "@types/node": "^24.0.4",
    "cspell": "^9.4.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
