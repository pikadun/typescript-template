{
  "name": "web-template",
  "version": "0.0.1",
  "description": "web template project",
  "type": "module",
  "scripts": {
    "dev": "APP_ENV=development node --experimental-strip-types --experimental-vm-modules scripts/start.ts",
    "build": "rsbuild build --config ./scripts/rsbuild.config.ts",
    "commitlint": "commitlint --config ./eng/.commitlintrc.json --edit",
    "cspell": "cspell --config ./eng/cspell.config.json --quiet .",
    "eslint": "eslint --config ./eng/eslint.config.js .",
    "lint": "npm run cspell && npm run eslint",
    "lint:staged": "lint-staged --config ./eng/.lintstagedrc.json",
    "postinstall": "husky || true",
    "type-check": "rm -rf ./node_modules/.cache/tsbuild && tsc --build ./tsconfig.json"
  },
  "author": "Dunn",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/platform-fastify": "^11.1.13",
    "@sequelize/core": "^7.0.0-alpha.47",
    "@sequelize/sqlite3": "^7.0.0-alpha.47"
  },
  "devDependencies": {
    "@camaro/eslint-config": "~0.1.2",
    "@camaro/tsconfig": "^1.2.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-vue": "^1.2.5",
    "@types/node": "^24.0.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "cspell": "^9.6.4",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "vuetify": "^3.11.8"
  }
}
